<% layout('layout/boilerplate.ejs') %>
<h1 class="color-scheme-text custom-bg-grey text-center">
  Visualisierungen der Daten
</h1>
<div class="darker-bg-color">
  <div class="container text-center">
    <div id="accordion">
      <div class="section-background-1">
        <div class="row">
          <h2 class="color-scheme-text mt-4">
            Fakten über die Kunst auf dem Campus:
          </h2>
        </div>
        <!--First Card:-->
        <div class="row first-row">
          <div class="col col-lg-8">
            <div class="card my-3" id="visualization_card_1">
              <div class="card-header" id="headingOne">
                <h5 class="mb-0">
                  <button
                    class="btn btn-link"
                    data-toggle="collapse"
                    data-target="#collapseOne"
                    aria-expanded="true"
                    aria-controls="collapseOne"
                  >
                    Wann sind die Kunst am Bau-Werke auf dem Campus entstanden?
                  </button>
                </h5>
              </div>

              <div
                id="collapseOne"
                class="collapse show"
                data-card-number="1"
                aria-labelledby="headingOne"
                data-parent="#accordion"
              >
                <div class="card-body">
                  <div
                    class="flourish-embed flourish-gantt"
                    data-src="visualisation/20429865"
                    onerror="this.outerHTML = '<img src=\'/Visualisierungen_Screenshots/Visualisierungen_Flourish_Aufstellungszeiträume.jpg\' alt=\'Fallback Gantt Chart\' style=\'width: 100%;\' >'"
                  >
                    <script
                      src="https://public.flourish.studio/resources/embed.js"
                      onerror="this.outerHTML = '<img src=\'/Visualisierungen_Screenshots/Visualisierungen_Flourish_Aufstellungszeiträume.jpg\' alt=\'Fallback Gantt Chart\' style=\'width: 100%;\' >'"
                    ></script>
                    <!-- Fallback Screenshot -->
                    <noscript>
                      <img
                        src="/Visualisierungen_Screenshots/Visualisierungen_Flourish_Aufstellungszeiträume.jpg"
                        width="100%"
                        alt="gantt visualization"
                      />
                    </noscript>
                  </div>
                </div>
              </div>
            </div>
          </div>

          <div class="col-12 col-lg-4 text-center">
            <img
              src="Illustrationen/toutes_directions.png"
              class="img-fluid custom-image"
              alt="ein Bild"
            />
            <div
              class="d-flex flex-row align-items-center justify-content-center my-3"
            >
              <p
                class="p-data-visualizations-text p-3"
                data-visualizations-id="1"
              ></p>
            </div>
          </div>
        </div>

        <!--Second Card:-->
        <div class="row middle-row">
          <div class="col col-lg-8 offset-lg-2">
            <div class="card my-3" id="visualization_card_2">
              <div class="card-header" id="headingTwo">
                <h5 class="mb-0">
                  <button
                    class="btn btn-link collapsed"
                    data-toggle="collapse"
                    data-target="#collapseTwo"
                    aria-expanded="false"
                    aria-controls="collapseTwo"
                  >
                    Wie ist das Verhältnis von Außen-/und Innenbereich?
                  </button>
                </h5>
              </div>
              <div
                id="collapseTwo"
                class="collapse"
                data-card-number="2"
                aria-labelledby="headingTwo"
                data-parent="#accordion"
              >
                <div class="card-body">
                  <div
                    class="flourish-embed flourish-chart"
                    data-src="visualisation/20447596"
                  >
                    <script src="https://public.flourish.studio/resources/embed.js"></script>
                    <noscript
                      ><img
                        src="https://public.flourish.studio/visualisation/20447596/thumbnail"
                        width="100%"
                        alt="chart visualization"
                    /></noscript>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="col-12 col-lg-4">
            <p
              class="p-data-visualizations-text p-3"
              data-visualizations-id="2"
            ></p>
          </div>
        </div>

        <!--Third Card:-->
        <div class="row mb-2 last-row">
          <div class="col-12 col-lg-4 text-center first-item">
            <img
              src="Illustrationen/wasserrelief_a.png"
              class="img-fluid custom-image"
              alt="ein Bild"
            />
            <p
              class="p-data-visualizations-text p-3"
              data-visualizations-id="3"
            ></p>
          </div>
          <div class="col-12 col-lg-8 text-center second-item">
            <div class="card my-3" id="visualization_card_3">
              <div class="card-header" id="headingThree">
                <h5 class="mb-0">
                  <button
                    class="btn btn-link collapsed"
                    data-toggle="collapse"
                    data-target="#collapseThree"
                    aria-expanded="false"
                    aria-controls="collapseThree"
                  >
                    Welche Materialien wurden verwendet?
                  </button>
                </h5>
              </div>
              <div
                id="collapseThree"
                class="collapse"
                data-card-number="3"
                aria-labelledby="headingThree"
                data-parent="#accordion"
              >
                <div class="card-body">
                  <div
                    class="flourish-embed flourish-word-cloud"
                    data-src="visualisation/20448421"
                  >
                    <script src="https://public.flourish.studio/resources/embed.js"></script>
                    <noscript
                      ><img
                        src="https://public.flourish.studio/visualisation/20448421/thumbnail"
                        width="100%"
                        alt="word-cloud visualization"
                    /></noscript>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>

      <div class="section-background-2">
        <!--Subtitle-->
        <div class="row">
          <h2 class="color-scheme-text mt-5 default-bg-color">
            Fakten über die Künstler:
          </h2>
        </div>
        <!--Fourth Card:-->
        <div class="row first-row">
          <div class="col-12 col-lg-8">
            <div class="card my-3" id="visualization_card_4">
              <div class="card-header" id="headingFour">
                <h5 class="mb-0">
                  <button
                    class="btn btn-link collapsed"
                    data-toggle="collapse"
                    data-target="#collapseFour"
                    aria-expanded="true"
                    aria-controls="collapseFour"
                  >
                    Wie heißen die Künstler?
                  </button>
                </h5>
              </div>
              <div
                id="collapseFour"
                class="collapse show"
                data-card-number="4"
                aria-labelledby="headingFour"
                data-parent="#accordion"
              >
                <div class="card-body">
                  <div
                    class="flourish-embed flourish-hierarchy"
                    data-src="visualisation/20444386"
                  >
                    <script src="https://public.flourish.studio/resources/embed.js"></script>
                    <noscript
                      ><img
                        src="https://public.flourish.studio/visualisation/20444386/thumbnail"
                        width="100%"
                        alt="hierarchy visualization"
                    /></noscript>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="col-12 col-lg-4">
            <img
              src="Illustrationen/stickman_campus.png"
              class="img-fluid custom-image"
              alt="Painting illustration"
            />
            <p
              class="p-data-visualizations-text p-3"
              data-visualizations-id="4"
            ></p>
          </div>
        </div>

        <!--Fifth Card:-->
        <div class="row middle-row">
          <div class="col-12 col-lg-8 offset-lg-2">
            <div class="card my-3" id="visualization_card_5">
              <div class="card-header" id="headingFive">
                <h5 class="mb-0">
                  <button
                    class="btn btn-link collapsed"
                    data-toggle="collapse"
                    data-target="#collapseFive"
                    aria-expanded="false"
                    aria-controls="collapseFive"
                  >
                    Woher kommen die Künstler?
                  </button>
                </h5>
              </div>
              <div
                id="collapseFive"
                class="collapse"
                data-card-number="5"
                aria-labelledby="headingFive"
                data-parent="#accordion"
              >
                <div class="card-body">
                  <div
                    class="flourish-embed flourish-bar-chart-race"
                    data-src="visualisation/20447741"
                  >
                    <script src="https://public.flourish.studio/resources/embed.js"></script>
                    <noscript
                      ><img
                        src="https://public.flourish.studio/visualisation/20447741/thumbnail"
                        width="100%"
                        alt="bar-chart-race visualization"
                    /></noscript>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="col-12 col-lg-4">
            <p
              class="p-data-visualizations-text p-3"
              data-visualizations-id="5"
            ></p>
          </div>
        </div>

        <!--Sixth Card:-->
        <div class="row mb-2 last-row">
          <div class="col-12 col-lg-4 text-center first-item">
            <img
              src="Illustrationen/stickman_graffiti.png"
              class="img-fluid custom-image"
              alt="Writing illustration"
            />
            <p
              class="p-data-visualizations-text p-3"
              data-visualizations-id="6"
            ></p>
          </div>
          <div class="col-12 col-lg-8 second-item">
            <div class="card my-3" id="visualization_card_6">
              <div class="card-header" id="headingSix">
                <h5 class="mb-0">
                  <button
                    class="btn btn-link collapsed"
                    data-toggle="collapse"
                    data-target="#collapseSix"
                    aria-expanded="false"
                    aria-controls="collapseSix"
                  >
                    Welches Geschlecht haben die Künstler?
                  </button>
                </h5>
              </div>
              <div
                id="collapseSix"
                class="collapse"
                data-card-number="6"
                aria-labelledby="headingSix"
                data-parent="#accordion"
              >
                <div class="card-body">
                  <div
                    class="flourish-embed flourish-pictogram"
                    data-src="visualisation/20443926"
                  >
                    <script src="https://public.flourish.studio/resources/embed.js"></script>
                    <noscript
                      ><img
                        src="https://public.flourish.studio/visualisation/20443926/thumbnail"
                        width="100%"
                        alt="pictogram visualization"
                    /></noscript>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>

        <div class="row">
          <h2 class="color-scheme-text mt-5">Fakten über die Interviewees:</h2>
        </div>
        <!--Seventh Card:-->
        <div class="row first-row">
          <div class="col-12 col-lg-8">
            <div class="card my-3" id="visualization_card_7">
              <div class="card-header" id="headingSeven">
                <h5 class="mb-0">
                  <button
                    class="btn btn-link collapsed"
                    data-toggle="collapse"
                    data-target="#collapseSeven"
                    aria-expanded="false"
                    aria-controls="collapseSeven"
                  >
                    Welches Geschlecht haben die Interviewees?
                  </button>
                </h5>
              </div>
              <div
                id="collapseSeven"
                class="collapse"
                data-card-number="7"
                aria-labelledby="headingSeven"
                data-parent="#accordion"
              >
                <div class="card-body">
                  <div
                    class="flourish-embed flourish-scatter"
                    data-src="visualisation/21051517"
                  >
                    <script src="https://public.flourish.studio/resources/embed.js"></script>
                    <noscript
                      ><img
                        src="https://public.flourish.studio/visualisation/21051517/thumbnail"
                        width="100%"
                        alt="pictogram visualization"
                    /></noscript>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="col-12 col-lg-4">
            <p
              class="p-data-visualizations-text p-3"
              data-visualizations-id="7"
            ></p>
          </div>
        </div>
      </div>
    </div>
  </div>

  <script type="module" src="/js/app.js"></script>
  <script type="module" src="/js/visualizations.js"></script>
</div>
